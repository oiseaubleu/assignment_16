<h1><%= t('.title') %> </h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label t('.email') %>
  <%= f.email_field :email %>
  <%= f.label t('.password')%>
  <%= f.password_field :password %>
   
<p><%= f.submit t('.butname'), id:'create-session' %></p>
<% end %>